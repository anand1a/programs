#PF-Prac-5
def count_digits_letters(sentence):
    result_list=[]
    sentence=sentence.strip()
    c=0 
    d=0
    for i in sentence:
        if(i.isalpha()):
            c=c+1
        elif(i.isdigit()):
            d=d+1
    result_list.append(c)
    result_list.append(d)
    return result_list

sentence="Infosys Mysore 570027"
print(count_digits_letters(sentence))
