#PF-Prac-6
def list123(nums):
    c=0
    for i in range(0,len(nums)):
        if(nums[i]==1):
            if(nums[i+1]==2):
                if(nums[i+2]==3):
                    c=c+1
        elif(nums[i]!=1):
            i=i+1
    if(c>0):
        return True
    else:
        return False
    

nums=[1, 1, 2, 4, 3]
print(list123(nums))
